	<sqlMap resource="com/zero2ipo/car/vipcoupon/bo/Vip_coupon_sql.xml"></sqlMap>
	<sqlMap resource="com/zero2ipo/car/choujiang/bo/Choujiang_result_sql.xml"></sqlMap>
